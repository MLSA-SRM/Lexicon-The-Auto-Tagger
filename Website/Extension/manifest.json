{
    "manifest_version": 2,
    "name": "High - Autotagger",
    "description": "Autotagger for article websites",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "http://localhost/*",
      "http://127.0.0.1:5000/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "popup.js"
        ]
      }
    ]
}